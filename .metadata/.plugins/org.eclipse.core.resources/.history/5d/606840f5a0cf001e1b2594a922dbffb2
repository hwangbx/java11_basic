package org.kh.java;

public class Variable4 {
	public static void main(String[] args) {
		String name = "황병찬";
		short age = 29;
		float heigt = 177.7f;
		float weight = 77.7f;
		
		/* 이스케이프 문자
		 \n : 줄 바꿈
		 \t : tab 띄우기
		 \\ : \을 출력
		 */
		String loc = "C:\\kim\\new\\temp";
		System.out.println(loc);
	}
}