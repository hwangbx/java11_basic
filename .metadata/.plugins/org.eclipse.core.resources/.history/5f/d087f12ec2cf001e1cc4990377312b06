package org.kh.java;

public class Operation3 {
	public static void main(String[] args) {
		int a = 0;
		a += 8; // a = a + 8 => 8
		System.out.printlna("a = "+a);
		a -= 3; // a = a - 3 => 5
		System.out.printlna("a = "+a);
		a *= 3; // a = a * 5 => 25
		System.out.printlna("a = "+a);
		a /= 4; // a = a \ 4 => 6
		
		
		
	}
}